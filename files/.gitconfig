[user]
	name = Renaud Tircher
	email = renaud.tircher@gmail.com

[color]
        diff = auto
        status = auto
        branch = auto
        interactive = auto

[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold

[color "status"]
        added = green
        changed = red
        untracked = yellow

[alias]
        st = status
[core]
	excludesfile = ~/.gitignore
[github]
        user = rtircher
[branch]
	autosetupmerge = true
[push]
	default = simple
[help]
	autocorrect = 1
